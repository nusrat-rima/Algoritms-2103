#include <iostream>
using namespace std;
int main()
{
  queue<int> q;
  q.push(10);
  q.push(15);
  q.push(20);
  cout << "Queue size :" << q.size;
  cout << "Front Element :" << q.front;
  cout << "Blck element :" << q.back;
